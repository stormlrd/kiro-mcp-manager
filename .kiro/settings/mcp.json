{
  "mcpServers": {
    "awslabs.cfn-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.cfn-mcp-server@latest"
      ],
      "env": {
        "AWS_PROFILE": "default"
      },
      "disabled": false,
      "autoApprove": [],
      "documentation_url": "https://awslabs.github.io/mcp/servers/cfn-mcp-server/",
      "category": "Infrastructure & Deployment"
    },
    "awslabs.amazon-bedrock-agentcore-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.amazon-bedrock-agentcore-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "DEBUG"
      },
      "disabled": false,
      "autoApprove": [],
      "documentation_url": "https://awslabs.github.io/mcp/servers/amazon-bedrock-agentcore-mcp-server/",
      "category": "AI & Machine Learning"
    },
    "awslabs.aws-api-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.aws-api-mcp-server@latest"
      ],
      "env": {
        "AWS_REGION": "us-east-1"
      },
      "disabled": false,
      "autoApprove": [],
      "documentation_url": "https://awslabs.github.io/mcp/servers/aws-api-mcp-server/",
      "category": "Getting Started with AWS"
    }
  }
}